{
  "name": "Don't Forget",
  "description": "Don't Forget adds reminders",
  "version": "0.1",
  "permissions": ["notifications", "storage", "alarms"],
  "background": {
    "scripts": ["bg_settings.js", "bg_omnibox.js", "bg_alarms.js", "bg_sync.js"],
    "persistent": false
  },
  "manifest_version": 2,
  "omnibox": {
    "keyword": "df"
  },
  "browser_action": {
    "default_icon": "images/logo_BW128.png",
    "default_popup": "popup/DontForget.html"
  },
  "icons": {
    "128": "images/logo128.png"
  }
}
