{
  "name": "Don't Forget",
  "description": "Don't Forget adds reminders",
  "version": "0.1",
  "permissions": ["notifications","storage"],
  "background": {
    "scripts": ["bg_omnibox.js"],
    "persistent": false
  },
  "manifest_version": 2,
  "omnibox": {
    "keyword": "df"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "DontForget.html"
  }
}
