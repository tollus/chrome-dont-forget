{
  "name": "Don't Forget",
  "description": "Don't Forget adds reminders",
  "version": "0.1",
  "permissions": ["notifications", "storage", "alarms"],
  "background": {
    "scripts": ["bg_omnibox.js", "bg_alarms.js"],
    "persistent": false
  },
  "manifest_version": 2,
  "omnibox": {
    "keyword": "df"
  },
  "browser_action": {
    "default_icon": "DontForgetBW.png",
    "default_popup": "DontForget.html"
  },
  "icons": {
    "128": "DontForget.png"
  }
}
